<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AndroidMenuDSL" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#2979FF</item>
        <item name="colorPrimaryVariant">#0091EA</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FF9100</item>
        <item name="colorSecondaryVariant">#FF6D00</item>
        <item name="colorOnSecondary">#000000</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >#1B3DFF</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.AndroidMenuDSL.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AndroidMenuDSL.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AndroidMenuDSL.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>